<div class="container">


  <div class="card-small">
    <% @lists.each do |list| %>
      <div class="card-small-information shadow" >
        <div class = "card-small-picture">
          <%= cl_image_tag list.photo.key, height: 300, width: 400, crop: :fill %>
        </div>
        <div class="card-small-name">
          <%= link_to list.name.capitalize, list_path(list) %>
        </div>
      </div>
    <% end %>
  </div>


</div>
